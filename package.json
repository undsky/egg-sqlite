{
  "name": "egg-database-sqlite",
  "version": "1.0.0",
  "description": "Egg plugin for sqlite",
  "eggPlugin": {
    "name": "sqlite"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "sqlite",
    "sqlite3"
  ],
  "dependencies": {
    "better-sqlite3": "^7.4.5"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.30.0",
